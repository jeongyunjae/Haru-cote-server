{
  "name": "haru-cote-server",
  "version": "1.0.0",
  "description": "koa.js",
  "main": "app.js",
  "scripts": {
    "start:pm2": "pm2 start run.config.json",
    "start": "cross-env NODE_ENV=production node ./dist/app.js -i 0",
    "dev": "cross-env NODE_ENV=local nodemon --watch 'src/**/*' -e ts --exec ts-node -r tsconfig-paths/register --files ./src/app.ts",
    "restart": "yarn start:dev",
    "kill": "yarn pm2 kill",
    "monit": "yarn pm2 monit",
    "path": "tscpaths -p tsconfig.json -s ./src -o ./dist",
    "build": "tsc --p tsconfig.json && npm run path"
  },
  "license": "MIT",
  "dependencies": {
    "@koa/cors": "3.1.0",
    "axios": "0.21.1",
    "bcrypt": "5.0.1",
    "cors": "2.8.5",
    "cross-env": "7.0.3",
    "database": "0.0.2",
    "dotenv": "9.0.2",
    "joi": "17.4.0",
    "jsonwebtoken": "8.5.1",
    "koa": "2.15.0",
    "koa-body": "4.2.0",
    "koa-bodyparser": "4.3.0",
    "koa-csrf": "3.0.8",
    "koa-morgan": "1.0.1",
    "koa-router": "10.0.0",
    "moment": "2.29.1",
    "moment-timezone": "0.5.33",
    "open": "8.2.1",
    "opn": "6.0.0",
    "path": "0.12.7",
    "pg": "8.6.0",
    "pm2": "4.5.6",
    "q": "1.5.1",
    "qs": "6.10.1",
    "request": "2.88.2",
    "script-tags": "0.1.3",
    "tscpaths": "0.0.9",
    "util": "0.12.4"
  },
  "devDependencies": {
    "@types/axios": "0.14.0",
    "@types/bcrypt": "5.0.0",
    "@types/cors": "2.8.10",
    "@types/joi": "17.2.3",
    "@types/jsonwebtoken": "8.5.1",
    "@types/koa": "2.13.1",
    "@types/koa-bodyparser": "4.3.12",
    "@types/koa-csrf": "3.0.1",
    "@types/koa-router": "7.4.2",
    "@types/koa__cors": "3.0.2",
    "@types/moment": "2.13.0",
    "@types/moment-timezone": "0.5.30",
    "@types/nodemailer": "6.4.2",
    "@types/pg": "8.6.0",
    "@types/q": "1.5.4",
    "@types/qs": "6.9.6",
    "@types/request": "2.48.5",
    "nodemon": "2.0.7",
    "ts-node": "9.1.1",
    "tsc-watch": "4.4.0",
    "tsconfig-paths": "3.9.0",
    "typescript": "4.2.4"
  }
}
